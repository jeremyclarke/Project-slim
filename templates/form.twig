{% extends "base.twig" %}

{% block title %}
    {{ formDetails['title'] }}
{% endblock %}

{% block content %}

    <div class="container">
        <div class="page-header">
            <h1>{{ formDetails['title'] }}</h1>
        </div>

        {% if formDetails['description'] is not empty %}
            <div class="well">
                <span>{{ formDetails['description']|raw }}</span>
            </div>
        {% endif %}

        <ul>
            {% for objects in objectsAll %}
                {% include "objects/#{objects.type}.twig" %}
                </br>
                </br>
            {% endfor %}
        </ul>

    </div>

{% endblock %}
