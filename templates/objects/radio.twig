{% if objects.required == '1' %}
    {% set required = 'required' %}
{% else %}
    {% set required = '' %}
{% endif %}

<input type="hidden" name="radio_{{ objects.ID }}" value="">

{% for option in objectSQL[objects.ID] %}

    {% if option[1] is empty %}
        <input type="radio" name="radio_{{ objects.ID }}" value="{{ option[0]|raw }}" {{ required }}> {{ option[0]|raw }}<br>
    {% else %}
        <input type="radio" name="radio_{{ objects.ID }}" value="{{ option[0]|raw }}" {{ required }}> {{ option[1]|raw }}<br>
    {% endif %}

{% endfor %}


